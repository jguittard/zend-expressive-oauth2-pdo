{
  "name": "jguittard/zend-expressive-oauth2-pdo",
  "description": "PDO mapper for Zend Expressive OAuth2 repositories",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julien Guittard",
      "email": "julien.guittard@me.com"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-hydrator": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "zendframework/zend-coding-standard": "~1.0.0",
    "jguittard/zend-expressive-oauth2": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Zend\\Expressive\\OAuth2\\Mapper\\Pdo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZendTest\\Expressive\\OAuth2\\Mapper\\Pdo\\": "test/"
    }
  },
  "scripts": {
    "check": [
      "@cs",
      "@test"
    ],
    "cs": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jguittard/zend-expressive-oauth2"
    }
  ]
}
